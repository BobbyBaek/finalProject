<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>
	
	<typeAliases>
		
		<typeAlias type="kr.co.project.member.model.dto.MemberDTO" alias="member"/>
		<typeAlias type="kr.co.project.chef.model.dto.ChefDTO" alias="chef"/>
		<typeAlias type="kr.co.project.recipe.model.dto.RecipeDTO" alias="recipe"/>
		
		<!-- 		 
		 <typeAlias type="kr.co.green.board.model.dto.FreeDTO" alias="free"/>
		<typeAlias type="kr.co.green.board.model.dto.InfoDTO" alias="info"/>
		<typeAlias type="kr.co.green.board.model.dto.NoticeDTO" alias="notice"/>
		<typeAlias type="kr.co.green.member.model.dto.MemberDTO" alias="member" />
		 -->
	</typeAliases>
	
	<mappers>
		<mapper resource="/mappers/member/member-mapper.xml"/>
		<mapper resource="/mappers/chef/chef-mapper.xml"/>	
		<mapper resource="/mappers/recipe/recipe-mapper.xml"/>	
		<mapper resource="/mappers/recipe/recipeIngredient-mapper.xml"/>
		<mapper resource="/mappers/recipe/recipeSequence-mapper.xml"/>
	</mappers>
</configuration>




