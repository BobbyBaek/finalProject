		<mapper resource="/mappers/recipe/recipeSequencePhoto-mapper.xml"/>
		<mapper resource="/mappers/recipe/recipeComment-mapper.xml"/>
		<mapper resource="/mappers/recipe/recipeScrap-mapper.xml"/>
		<mapper resource="/mappers/foodMate/foodMate-mapper.xml"/>
		<mapper resource="/mappers/foodMate/foodMateComment-mapper.xml"/>
		<mapper resource="/mappers/admin/admin-mapper.xml"/>
		<mapper resource="/mappers/admin/event-mapper.xml"/>	
	</mappers>
</configuration>




